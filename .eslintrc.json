{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "warn",
            2
        ],
        "quotes": [
            "warn"
        ],
        "semi": [
            "warn"
        ],
        "no-trailing-spaces": [
            "warn"
        ],
        "comma-spacing": [
            "warn"
        ],
        "no-multi-spaces": [
            "warn"
        ],
        "no-multiple-empty-lines": [
            "warn"
        ],
        "space-before-function-paren": [
            "warn"
        ],
        "space-before-blocks": [
            "warn"
        ],
        "space-in-parens": [
            "warn"
        ],
        "space-infix-ops": [
            "warn"
        ],
        "space-unary-ops": [
            "warn"
        ],
        "spaced-comment": [
            "warn"
        ],
        "no-whitespace-before-property": [
            "warn"
        ],
        "keyword-spacing": [
            "warn"
        ],
        "brace-style": [
            "warn"
        ],
        "no-mixed-spaces-and-tabs": [
            "warn"
        ],
        "no-tabs": [
            "warn"
        ],
        "no-irregular-whitespace": [
            "warn"
        ]
    }
}
